{
  "name": "ipyradiant-dev",
  "version": "0.0.0",
  "license": "BSD-3-Clause",
  "private": true,
  "devDependencies": {
    "prettier": "^2.0.5"
  },
  "scripts": {
    "bootstrap": "jlpm deps && jlpm lint",
    "deps": "jlpm --offline --ignore-optional || jlpm --prefer-offline --ignore-optional || jlpm --ignore-optional",
    "lint": "jlpm lint:prettier",
    "lint:prettier": "prettier --list-different --write \"*.{yml,md,json}\" \".github/**/*.yml\""
  }
}
